{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"rootDir": ".",
		"outDir": "dist",
		"strict": true, /* enable all strict type-checking options */
		"resolveJsonModule": true,
		"esModuleInterop": true ,
		"skipLibCheck": true
	},
    "exclude": [
        ".vscode",
        "node_modules",
        "dist",
		"./src/test/**/*",
    ]	
}